ifeq ($(OUTPUT_DIR),)
$(error Please define OUTPUT_DIR)
endif

include $(OUTPUT_DIR)/mk/pre.mk

ifeq ($(MSP_MON_LOG_IMPL),)
$(error Please define MSP_MON_LOG_IMPL)
endif

$(eval $(call ADD_HDRS_ALL,log,,generic/include))

$(eval $(call ADD_LIB,log,$(MSP_MON_LOG_IMPL)))

include $(OUTPUT_DIR)/mk/post.mk
